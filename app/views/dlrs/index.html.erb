<%= render  'layouts/navbar' %>

<p id="notice"><%= notice %></p>

<h1>Listing Dlrs</h1>

<table>
  <thead>
    <tr>
      <th>To</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dlrs.each do |dlr| %>
      <tr>
        <td><%= dlr.to %></td>
        <td><%= link_to 'Show', dlr %></td>
        <td><%= link_to 'Edit', edit_dlr_path(dlr) %></td>
        <td><%= link_to 'Destroy', dlr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dlr', new_dlr_path, :class => "button" %>
